public class Registro{
    private String user;
    private String pass;

    @PostConstruct
    Void cargar(){}

    @PostDestroy
    Void eliminar(){}

    public void RegistroListener(){
        try {
            Procesos.Registro(this.user, this.pass);
            Util.crearMensaje("Registro Exitoso",SEVERITY_INFO);
        } catch(Exception e){
            Util.crearMensaje("Registro fallido", SEVERITY_ERROR);
        }
    }
}